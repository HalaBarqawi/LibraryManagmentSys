<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxdatabase.FXML_newmemberController">
   <children>
      <AnchorPane id="AnchorPane" layoutY="-11.0" prefHeight="464.0" prefWidth="637.0" style="-fx-background-color: #009688;">
         <children>
            <VBox layoutX="79.0" layoutY="106.0" prefHeight="200.0" prefWidth="218.0" spacing="25.0">
               <children>
                  <TextField fx:id="id" blendMode="MULTIPLY" promptText="Memeber ID ">
                     <effect>
                        <DropShadow />
                     </effect>
                  </TextField>
                  <VBox prefHeight="135.0" prefWidth="220.0" spacing="25.0">
                     <children>
                        <TextField fx:id="FN" blendMode="MULTIPLY" prefHeight="12.0" prefWidth="219.0" promptText="First Name">
                           <effect>
                              <DropShadow />
                           </effect>
                        </TextField>
                        <TextField fx:id="MN" blendMode="MULTIPLY" promptText="Second Name">
                           <effect>
                              <DropShadow />
                           </effect>
                        </TextField>
                        <TextField fx:id="LN" blendMode="MULTIPLY" promptText="Last Name">
                           <effect>
                              <DropShadow />
                           </effect>
                        </TextField>
                     </children>
                  </VBox>
                  <TextField fx:id="emailAdd" blendMode="MULTIPLY" promptText="Email Address">
                     <effect>
                        <DropShadow />
                     </effect>
                  </TextField>
                  <TextField fx:id="phone" blendMode="MULTIPLY" promptText="Phone #">
                     <effect>
                        <DropShadow />
                     </effect>
                  </TextField>
               </children>
            </VBox>
            <VBox fx:id="price" layoutX="338.0" layoutY="106.0" prefHeight="200.0" prefWidth="220.0" spacing="25.0">
               <children>
                  <HBox prefHeight="200.0" prefWidth="218.0">
                     <children>
                        <CheckBox fx:id="femaleCheck" blendMode="MULTIPLY" mnemonicParsing="false" onAction="#genderCheck" prefHeight="21.0" prefWidth="103.0" text="Female" textFill="#393837">
                           <font>
                              <Font name="Cambria" size="18.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="maleCheck" blendMode="MULTIPLY" mnemonicParsing="false" onAction="#genderCheck" prefHeight="21.0" prefWidth="97.0" text="Male" textFill="#393837">
