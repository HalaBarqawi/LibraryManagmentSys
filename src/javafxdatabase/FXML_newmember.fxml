                              <Font name="Cambria" size="18.0" />
                           </font>
                        </CheckBox>
                     </children>
                  </HBox>
                  <DatePicker fx:id="DB" blendMode="MULTIPLY" prefHeight="31.0" prefWidth="232.0" promptText="Date Of Birth">
                     <effect>
                        <DropShadow />
                     </effect>
                  </DatePicker>
                  <DatePicker fx:id="DP" blendMode="MULTIPLY" prefHeight="31.0" prefWidth="228.0" promptText="Date of Participation">
                     <effect>
                        <DropShadow />
                     </effect>
                  </DatePicker>
                  <TextField fx:id="paidp" blendMode="MULTIPLY" prefHeight="31.0" prefWidth="220.0" promptText="The Price paid">
                     <effect>
                        <DropShadow />
                     </effect>
                  </TextField>
                  <TextField fx:id="ssn" blendMode="MULTIPLY" promptText="Attended By..">
                     <effect>
                        <DropShadow />
                     </effect>
                  </TextField>
               </children>
            </VBox>
            <Button fx:id="add" layoutX="453.0" layoutY="386.0" mnemonicParsing="false" onAction="#memberAction" prefHeight="31.0" prefWidth="100.0" style="-fx-background-color: #f44336; -fx-background-radius: 50;" text="ADD">
